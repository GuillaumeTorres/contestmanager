{% spaceless %}{% trans_default_domain("Admin") %}
    {% for flash_message in app.session.flashBag.get('notice') %}
        <div class="flash-notice">
            {{ flash_message }}
        </div>
    {% endfor %}
    <div class="box">
        <div class="box-header">
            <h3 class="box-title">{{ "dashboard.tournament"|trans({}, 'Admin') }}</h3>
        </div>
        <div class="box-body">
            <a href="{{ path('tournament') }}" class="btn btn-primary">{{ "dashboard.start"|trans({}, 'Admin') }}</a>
        </div>
    </div>
{% endspaceless %}
